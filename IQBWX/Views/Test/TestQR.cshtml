
@{
    ViewBag.Title = "TestQR";
    Layout = "~/Views/Shared/_O2OLayout.cshtml";
}

<script type="text/javascript">

    $(function () {
        DoCreateQR = function () {
            var url = "/api/wx/CreateShuDanInviteQR?sId=100&ss=SD";
            $.ajax({
                type: 'post',
                url: url,
                data: { "sId": 100, "ss": "SD"},
                success: function (res) {
                    alert(res.TargetUrl);
                },
            });
        }
        
    })
</script>
<input type="button" id="btnCreateQR" value="CreateQR" onclick="DoCreateQR();" />
